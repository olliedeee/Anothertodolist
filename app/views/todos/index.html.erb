<center><h1> All Todos</h1></center>
<br>
  <div class="row">
    <div class="col-md-12">
      <div class="row headings">
        <div class="col-md-2">Name </div>
        <div class="col-md-2">Description </div>
        <div class="col-md-1">ID </div>
        <div class="col-md-3">Creator </div>
        <div class="col-md-1">View </div>
        <div class="col-md-1">Edit/Delete </div>
      </div>
      <br>
      
    <% @nameitanything.each do |tdi| %>
      <div class="row headings">
        <div class="col-md-2"><%= tdi.name %> </div>
        <div class="col-md-2"><%= tdi.description %> </div>
        <div class="col-md-1"><%= tdi.id %> </div>
        <div class="col-md-3"> <%= tdi.user.email %> </div>
        <div class="col-md-1"><%= link_to "Show", todo_path(tdi) %> </div>
        <div class="col-md-2"><%= link_to "Edit", edit_todo_path(tdi) %> / <%= link_to "Delete", todo_path(tdi),
        method: :delete, data: {confirm: "are you sure?"} %> </div>
      </div>
    <% end %>
